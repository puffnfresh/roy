// Caution: this example horribly breaks the monad laws.
// See Hacker News for more details:
// http://news.ycombinator.com/item?id=3277360

let traceMonad = {
  return: \x ->
    console.log "Return:" x
    x
  bind: \x f ->
    console.log "Binding:" x
    f x
}

console.log (do traceMonad
  bind w = 1
  let x = 2
  bind y = 3
  bind z = 4
  return w + x + y + z
)
